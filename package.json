{
  "name": "skills-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-knobs": "^3.2.12",
    "ajv": "^5.2.3",
    "apollo-client": "^1.9.3",
    "autoprefixer": "7.1.5",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.1.0",
    "css-loader": "0.28.7",
    "cucumber": "^3.0.4",
    "dev": "^0.1.3",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "graphql-tag": "^2.4.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "lint-staged": "^4.2.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.7",
    "prettier-standard": "^7.0.1",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-apollo": "^1.4.15",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "style-loader": "0.19.0",
    "styled-components": "^2.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "CI=true yarn test",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "prettier-standard --fix",
    "test": "node scripts/test.js --env=jsdom",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c .storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier-standard",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "jest-styled-components": "^4.7.0",
    "storybook-addon-jsx": "^4.2.0",
    "storybook-addon-specifications": "^2.1.1",
    "storybook-chrome-screenshot": "^0.6.0",
    "storybook-readme": "^3.0.6"
  }
}
